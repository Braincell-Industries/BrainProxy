{
  "name": "brainproxy",
  "version": "1.1.7",
  "description": "Custom built proxy with a ton of features, made for Hypixel!",
  "main": "dist/index.js",
  "bin": "dist/index.js",
  "license": "Attribution-NonCommercial-ShareAlike 4.0 International (CC BY-NC-SA 4.0)",
  "author": "ItzMarni",
  "homepage": "https://github.com/Braincell-Industries/BrainProxy#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Braincell-Industries/BrainProxy.git"
  },
  "scripts": {
    "start": "node dist/index.js",
    "build": "npx tsc",
    "clean": "if exist dist rd /s /q dist",
    "dev": "npm run build && node dist/index.js",
    "watch": "npm run build && npx tsc -w",
    "lint": "eslint \"**/*.{js,ts,tsx}\" --quiet --fix",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx}\"",
    "pkg": "npm run build && npx pkg ."
  },
  "devDependencies": {
    "@types/node": "18.16.1",
    "@typescript-eslint/eslint-plugin": "^5.59.7",
    "@typescript-eslint/parser": "^5.59.7",
    "eslint": "^8.41.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^4.2.1",
    "prettier": "^2.8.8",
    "typescript": "^5.0.4"
  },
  "dependencies": {
    "chalk": "4.1.0",
    "js-yaml": "^4.1.0",
    "path": "^0.12.7",
    "prismarine-chat": "^1.8.0",
    "prismarine-proxy": "^1.1.4"
  },
  "pkg": {
    "scripts": "dist/**/*.js",
    "assets": [
      "node_modules/minecraft-packets/minecraft-packets/data/pc/1.8",
      "dist/modules",
      "dist/commands",
      "package.json"
    ],
    "targets": [
      "node18-win-x64"
    ],
    "outputPath": "pkg"
  },
  "keywords": [
    "proxy",
    "minecraft",
    "hypixel",
    "hypixel-proxy",
    "minecraft-proxy",
    "brainproxy"
  ]
}
