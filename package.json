    "eslint-plugin-prettier": "^4.2.1",
    "prettier": "^2.8.8",
    "typescript": "^5.0.4"
  },
  "dependencies": {
    "@iarna/toml": "^2.2.5",
    "chalk": "^5.3.0",
    "path": "^0.12.7",
